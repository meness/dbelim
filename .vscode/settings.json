{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "autoBarrel.formatting.useSingleQuotes": true,
  "cSpell.enabledFileTypes": {
    "al": false,
    "asciidoc": false,
    "bat": false,
    "c": false,
    "clojure": false,
    "coffeescript": false,
    "cpp": false,
    "csharp": false,
    "css": true,
    "dart": false,
    "diff": false,
    "dockercompose": false,
    "dockerfile": false,
    "dotenv": true,
    "elixir": false,
    "erlang": false,
    "fsharp": false,
    "git-commit": false,
    "git-rebase": false,
    "github-actions-workflow": true,
    "go": false,
    "graphql": false,
    "groovy": false,
    "handlebars": false,
    "haskell": false,
    "html": true,
    "ignore": true,
    "ini": false,
    "jade": false,
    "java": false,
    "javascript": true,
    "javascriptreact": true,
    "json": true,
    "jsonc": true,
    "julia": false,
    "jupyter": false,
    "latex": false,
    "less": false,
    "log": true,
    "lua": false,
    "makefile": false,
    "markdown": true,
    "objective-c": false,
    "perl": false,
    "perl6": false,
    "php": false,
    "plaintext": true,
    "powershell": false,
    "properties": true,
    "pug": false,
    "python": false,
    "r": false,
    "razor": false,
    "restructuredtext": false,
    "ruby": false,
    "rust": false,
    "scala": false,
    "scminput": false,
    "scss": false,
    "shaderlab": false,
    "shellscript": false,
    "sql": false,
    "swift": false,
    "text": false,
    "typescript": true,
    "typescriptreact": true,
    "vb": false,
    "vitest-snapshot": true,
    "vue": false,
    "xml": true,
    "xsl": false,
    "yaml": true
  },
  "cSpell.language": "en-US,lorem",
  "cSpell.words": [
    "Chinmaya",
    "commitlint",
    "consts",
    "drgit",
    "drgitai",
    "Eskandari",
    "flexbox",
    "heroui",
    "lockb",
    "nextui",
    "packagejson",
    "reactflow",
    "xyflow"
  ],
  "css.validate": false,
  "editor.autoClosingQuotes": "always",
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.organizeImports": "always"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.foldingImportsByDefault": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.linkedEditing": true,
  "editor.quickSuggestions": {
    "strings": true
  },
  "eslint.lintTask.enable": true,
  "eslint.probe": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "yaml"
  ],
  "files.autoSave": "afterDelay",
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.next": true,
    "**/.vscode": true,
    "**/node_modules": true
  },
  "files.insertFinalNewline": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.next/**": true,
    "**/node_modules/*/**": true
  },
  "git.autoStash": true,
  "git.autofetch": true,
  "git.closeDiffOnOperation": true,
  "git.enableSmartCommit": true,
  "git.repositoryScanIgnoredFolders": [
    "node_modules",
    ".next",
    ".husky",
    ".vscode",
    "types",
    "public",
    "src"
  ],
  "githubIssues.createIssueTriggers": ["TODO", "BUG", "FIX"],
  "githubPullRequests.defaultMergeMethod": "squash",
  "githubPullRequests.ignoredPullRequestBranches": [
    "main"
  ],
  "i18n-ally.displayLanguage": "en-US",
  "i18n-ally.enabledFrameworks": ["react-i18next"],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.localesPaths": "public/locales",
  "i18n-ally.namespace": true,
  "i18n-ally.readonly": false,
  "i18n-ally.sourceLanguage": "en-US",
  "i18n-ally.usage.derivedKeyRules": ["{key}_other", "{key}_one", "{key}_zero"],
  "javascript.suggestionActions.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prettier.documentSelectors": [
    "**/*.svg"
  ],
  "scss.validate": false,
  "search.exclude": {
    "**/.next": true,
    "**/node_modules": true
  },
  "tailwindCSS.files.exclude": ["**/.git/**", "**/node_modules/**", "**/.hg/**", "**/.next/**"],
  "todo-tree.filtering.excludeGlobs": ["node_modules", ".next"],
  "todo-tree.general.automaticGitRefreshInterval": 60,
  "todo-tree.general.tags": ["BUG", "FIX", "TODO"],
  "todo-tree.highlights.backgroundColourScheme": ["red", "orange", "blue"],
  "todo-tree.highlights.foregroundColourScheme": ["white", "white", "white"],
  "todo-tree.highlights.useColourScheme": true,
  "typescript.preferences.quoteStyle": "single",
  "typescript.tsdk": "node_modules/typescript/lib"
}
